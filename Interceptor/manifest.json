{
  "manifest_version": 2,

  "name": "Inteceptor",
  "description": "Inteceptor",
  "version": "1.0",
  "background": {
      "page": "background.html"
  },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html",
   "default_title": "Inteceptor"
  },
  "permissions": [
   "activeTab",
   "webRequest",
    "webRequestBlocking",
    "contextMenus",
    "*://*/"
   ]
}